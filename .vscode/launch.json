{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter macOS (ClojureDart)",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "clj",
      "runtimeArgs": [
        "-M:cljd",
        "flutter",
        "-d", "macos",
        "--enable-impeller"
      ],
      "cwd": "${workspaceFolder}/example",
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
